{
  "name": "project-cards",
  "version": "1.0.0",
  "description": "Project Cards App",
  "author": "Jasper van der Linden",
  "private": true,
  "scripts": {
    "nuxt:lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "nuxt:precommit": "yarn nuxt:lint",
    "nuxt:test": "jest",
    "nuxt:dev": "nuxt dev",
    "nuxt:build": "nuxt build",
    "nuxt:start": "nuxt start",
    "nuxt:generate": "nuxt generate",
    "now-build": "nuxt generate",
    "build": "tsc -p server/tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nodemon --config=server/nodemon.json",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --config server/ormconfig.json",
    "start:dev": "concurrently --handle-input \"wait-on server/dist/main.js && nodemon\" \"tsc -w -p server/tsconfig.build.json\" ",
    "start:debug": "nodemon --config server/nodemon-debug.json",
    "prestart:prod": "rimraf server/dist && yarn nuxt:build",
    "start:prod": "node server/dist/main.js",
    "lint": "tslint -p server/tsconfig.json -c server/tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./server/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^6.3.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.2",
    "@nestjs/websockets": "^6.3.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "autoprefixer": "^9.6.0",
    "class-validator": "^0.9.1",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "mysql": "^2.17.1",
    "nuxt": "^2.0.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "socket.io-client": "^2.2.0",
    "swagger-ui-express": "^4.0.6",
    "typeorm": "^0.2.18",
    "typeorm-seeding": "^1.0.0-rc.3",
    "vue-property-decorator": "^8.1.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.12",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "supertest": "^3.4.1",
    "tailwindcss": "^1.0.0",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "vue-jest": "^3.0.3",
    "wait-on": "^3.2.0"
  }
}
