    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "supertest": "^3.4.1",
    "tailwindcss": "^1.0.0",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.4.3",
    "vue-jest": "^3.0.3",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.3.5"
  }
