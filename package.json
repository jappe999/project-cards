{
  "name": "project-cards",
  "version": "1.0.0",
  "description": "Project Cards App",
  "author": "Jasper van der Linden",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "cross-env NODE_ENV=production node dist/server/main.js",
    "start:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build": "cross-env NODE_ENV=production npm run clean && nuxt build && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint -p tsconfig.json --fix",
    "clean": "rimraf dist",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --config server/ormconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./server/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^6.3.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.2",
    "@nestjs/websockets": "^6.3.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "autoprefixer": "^9.6.0",
    "class-validator": "^0.9.1",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "mysql": "^2.17.1",
    "nuxt": "^2.0.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "socket.io-client": "^2.2.0",
    "swagger-ui-express": "^4.0.6",
    "typeorm": "^0.2.18",
    "typeorm-seeding": "^1.0.0-rc.3",
    "vue-property-decorator": "^8.1.1",
    "vuex-class": "^0.3.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@nestjs/testing": "^6.0.0",
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.12",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
