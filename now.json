{
  "version": 2,
  "env": {
    "production": "true",
    "PORT": "80",
    "HOST": "project.jappe1999.now.sh",
    "WS_URL": "https://project.jappe1999.now.sh"
  },
  "builds": [{
      "src": "client/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "server/dist/src/main.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [{
      "src": "/api/(.*)",
      "dest": "/server/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
